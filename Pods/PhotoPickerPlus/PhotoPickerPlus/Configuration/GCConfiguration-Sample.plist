<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
  <dict>
  	<key>chute-sdk</key>
  	<dict>
  		<key>app_id</key>
  		<string>YOUR_CLIENT_ID</string>
  		<key>app_secret</key>
  		<string>YOUR_CLIENT_SECRET</string>
  	</dict>
  	<key>photopicker</key>
  	<dict>
  		<key>configuration_url</key>
  		<string>https://dl.dropboxusercontent.com/u/23635319/ChuteAPI/config.json</string>
  		<key>services</key>
  		<array>
  			<string>facebook</string>
  			<string>google</string>
  			<string>googledrive</string>
  			<string>instagram</string>
  			<string>flickr</string>
  			<string>picasa</string>
  			<string>dropbox</string>
  			<string>skydrive</string>
  		</array>
  		<key>local_features</key>
  		<array>
  			<string>take_photo</string>
  			<string>last_taken_photo</string>
  			<string>camera_photos</string>
  		</array>
